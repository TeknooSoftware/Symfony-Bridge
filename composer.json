{
    "name": "teknoo/bridge-phpdi-symfony",
    "type": "library",
    "description": "Integrates PHP-DI to Symfony, Fork from php-di/symfony-bridge",
    "homepage": "http://php-di.org/doc/frameworks/symfony2.html",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Teknoo\\DI\\SymfonyBridge\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Teknoo\\DI\\SymfonyBridge\\Test\\": "tests/"
        }
    },
    "require": {
        "php": "~7.4",
        "php-di/php-di": "~6.0",
        "symfony/dependency-injection": "~4.4||~5.1",
        "symfony/http-kernel": "~4.4||~5.1",
        "symfony/proxy-manager-bridge": "~4.4||~5.1",
        "symfony/config": "~4.4||~5.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "~9.2",
        "squizlabs/php_codesniffer": "~3.5",
        "pdepend/pdepend": "~2.8",
        "sebastian/phpcpd": "~6.0",
        "phploc/phploc": "~7.0",
        "phpstan/phpstan": "^0.12.39",
        "symfony/filesystem": "~4.4||~5.1",
        "symfony/yaml": "~4.4||~5.1",
        "symfony/error-handler": "~4.4||~5.1"
    }
}
